<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>AnksDB</title>
<meta name="keywords" content="" />
<meta name="description" content="" />

{% block stylesheets %}
    <link href="{{ asset('bundles/lfpstructuralanksmain/css/default.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/lfpstructuralanksmain/css/fonts.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/lfpstructuralanksmain/css/main.css') }}" type="text/css" rel="stylesheet" />
{% endblock %}
    
{% block javascripts %}
    <script src="{{ asset('bundles/lfpstructuralanksmain/js/jquery-1.7.1.min.js ') }}" type="text/javascript"></script>    
    <script src="{{ asset('bundles/lfpstructuralanksmain/js/jquery.slidertron-1.3.js ') }}" type="text/javascript"></script>    
{% endblock %}

<link href="http://fonts.googleapis.com/css?family=Source+Sans+Pro:200,300,400,600,700,900" rel="stylesheet" />

<!--[if IE 6]><link href="default_ie6.css" rel="stylesheet" type="text/css" /><![endif]-->

</head>
<body>
<div id="header-wrapper">
	<div id="header" class="container">
		<div id="logo">
			{#<span class="icon icon-group"></span>#}
			<h1><a href="#">AnksDB</a></h1>
			<span>Ankyrin Repeat Database</span> </div>
		</div>
		{#<div id="triangle-up"></div>#}
	</div>
</div>
<div id="menu-wrapper">
		<div id="menu">
			<ul>
                            {% if current is not defined %}
                                {% set current = 'Home' %}
                            {% endif %}

                            <li {% if current == 'Home'%}class="current_page_item"{%endif%}><a href="{{path('ankyrins_home')}}" accesskey="1" title="">Home</a></li>
                            <li {% if current == 'Browse'%}class="current_page_item"{%endif%}><a href="{{path('ankyrins_browse')}}" accesskey="2" title="">Browse</a></li>
                            <li {% if current == 'About'%}class="current_page_item"{%endif%}><a href="#" accesskey="3" title="">About</a></li>
                            <li {% if current == 'Contact'%}class="current_page_item"{%endif%}><a href="#" accesskey="4" title="">Contact</a></li>
			</ul>
		</div>
</div>
<div id="wrapper">
	<div id="featured-wrapper">
		<div class="container">
			<div class="main-box">
			{%block container%}
                        {%endblock%}
			</div>		
		</div>
	</div>
</div>
<div id="copyright" class="container">
	<p>Copyright (c) 2013 Sitename.com. All rights reserved. | Photos by <a href="http://fotogrph.com/">Fotogrph</a> | Design by <a href="http://www.freecsstemplates.org/" rel="nofollow">FreeCSSTemplates.org</a>.</p>
</div>
</body>
</html>
