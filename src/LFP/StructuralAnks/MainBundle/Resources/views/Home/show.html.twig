{%extends "LFPStructuralAnksMainBundle::base.html.twig"%}

{%set current = 'Home'%}
    
{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('bundles/lfpstructuralanksmain/js/highcharts.js ') }}" type="text/javascript"></script>    
    <script src="{{ asset('bundles/lfpstructuralanksmain/js/modules/exporting.js ') }}" type="text/javascript"></script>    
    <script src="{{ asset('bundles/lfpstructuralanksmain/jmol/jmol.js') }}" type="text/javascript"></script>
    {% for op in jscharts%}
        {% include 'LFPStructuralAnksMainBundle:Home:jscharts.html.twig' with {'options':op}%}
    {% endfor%}
{% endblock %}



{%block container%}
<p>Pdb: {{pdbId}}</p>
<div id="jmol">
        <script>
            jmolInitialize("{{ asset('bundles/lfpstructuralanksmain/jmol/JmolApplet0.jar') }}");
            jmolSetCallback("UseCommandThread","true");
            jmolSetParameter("UseCommandThread","true");
        </script>
        <object name="jmol" id="jmol" code="JmolApplet" archive="{{ asset('bundles/lfpstructuralanksmain/jmol/JmolApplet0.jar') }}" width="400" height="333" type="application/x-java-applet">
            <param name="progressbar" value="true">
            <param name="bgcolor" value="#000000">
            <param name="style" value="wireframe">
            <param name="script" value="load {{ asset('bundles/lfpstructuralanksmain/jmol/anksdb/Frustrations/1A5E_A.pdb.done/renum_1A5E_A.pdb') }}; {{ 'bundles/lfpstructuralanksmain/jmol/anksdb/Frustrations/1A5E_A.pdb.done/test.jmol'|getContents }}">
            <param name="wireframeRotation" value="false">
            <param name="perspectiveDepth" value="false">
        </object>
</div>
<div id="charts" style="min-width: 310px; height: 400px; margin: 0 auto"></div>

{%endblock%}
