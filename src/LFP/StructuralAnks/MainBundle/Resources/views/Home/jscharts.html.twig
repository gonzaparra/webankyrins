
    <script type="text/javascript">
$(function () {
        $('#chart').highcharts({
            title: {
                text: 'Gráficos comparativos',
                x: -20 //center
            },
            subtitle: {
                text: 'Source: WorldClimate.com',
                x: -20
            },
            xAxis: {
                categories: {{options.xAxisCategories}}
            },
            yAxis: {
                title: {
                    text: 'Temperature (°C)'
