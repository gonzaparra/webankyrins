
    <script type="text/javascript">
$(function () {
        $('#chart').highcharts({
            chart: {
                zoomType: 'x'
            },
            title: {
                text: 'Residues Properties',
                x: -20 //center
            },
            {#subtitle: {
                text: 'Source: WorldClimate.com',
                x: -20
            },#}
            xAxis: {
                categories: {{options.xAxisCategories|raw}},
                {#min: 10,
                max: 40#}
            },
            scrollbar: {
                enabled: true
            },
            yAxis: [{ // Primary yAxis
                labels: {
                    formatter: function() {
                        return this.value +'Å';
                    },
                    style: {
                        color: '#89A54E'
                    }
                },
                title: {
                    text: 'Angstrom',
                    style: {
                        color: '#89A54E'
                    }
                },
                opposite: true
    
            }, { // Secondary yAxis
                gridLineWidth: 0,
                labels: {
                    formatter: function() {
                        return this.value +' E';
                    },
                    style: {
                        color: '#4572A7'
                    }
                },
                title: {
                    text: 'Energy',
                    style: {
                        color: '#4572A7'
                    }
                },
{#                        opposite: true#}
    
            }],
            tooltip: {
                shared: true
            },
            legend: {
                layout: 'vertical',
                align: 'left',
                x: 120,
                verticalAlign: 'top',
                y: 80,
                floating: true,
                backgroundColor: '#FFFFFF'
            },
            series: [{
                name: 'Angstrom',
                color: '#89A54E',
                data: {{options.sasa}},
                tooltip: {
                    valueSuffix: ' Å :D'
                }
            }, {
                name: 'Energy',
                color: '#4572A7',
                data: {{options.energy}},
                tooltip: {
                    valueSuffix: ' E'
                }
            }]
        });
    });
    
    </script>